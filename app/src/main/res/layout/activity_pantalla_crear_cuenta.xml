<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PantallaCrearCuenta"
    android:background="#000000"
    >

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="149dp"
        android:layout_height="135dp"
        android:layout_marginTop="60dp"
        app:layout_constraintBottom_toTopOf="@+id/editTextTNombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/ic_launcher" />

    <EditText
        android:id="@+id/editTextTelefono"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="phone"
        android:hint="Telefono"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toTopOf="@+id/textViewCCorreo"
        app:layout_constraintEnd_toEndOf="@+id/editTextCorreo"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textViewTelefono"
        app:layout_constraintTop_toBottomOf="@+id/textViewCRContraseña"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/editTextRepContraseña"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPassword"
        android:hint="Contraseña"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toTopOf="@+id/editTextTelefono"
        app:layout_constraintEnd_toEndOf="@+id/editTextTelefono"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textViewTelefono"
        app:layout_constraintTop_toBottomOf="@+id/textViewCContraseña"
        app:layout_constraintVertical_bias="0.194" />

    <EditText
        android:id="@+id/editTextContraseña"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPassword"
        android:hint="Contraseña"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toTopOf="@+id/editTextRepContraseña"
        app:layout_constraintEnd_toEndOf="@+id/editTextRepContraseña"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTNombre"
        app:layout_constraintVertical_bias="0.285" />

    <TextView
        android:id="@+id/textViewNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="14dp"
        android:text="Nombre Completo"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toTopOf="@+id/textViewCContraseña"
        app:layout_constraintEnd_toStartOf="@+id/editTextTNombre"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.213" />

    <EditText
        android:id="@+id/editTextTNombre"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginTop="184dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Nombre y Apellido"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toTopOf="@+id/textViewCContraseña"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.648"
        app:layout_constraintStart_toEndOf="@+id/textViewCContraseña"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.443" />

    <EditText
        android:id="@+id/editTextCorreo"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:hint="Correo Electronico"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toBottomOf="@+id/textViewCCorreo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTelefono"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textViewCContraseña"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="305dp"
        android:text="Contraseña"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toTopOf="@+id/textViewCRContraseña"
        app:layout_constraintEnd_toStartOf="@+id/editTextContraseña"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.444" />

    <TextView
        android:id="@+id/textViewCRContraseña"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="415dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="50dp"
        android:text="Repetir Contraseña"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toTopOf="@+id/textViewTelefono"
        app:layout_constraintEnd_toStartOf="@+id/editTextRepContraseña"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textViewTelefono"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.357" />

    <TextView
        android:id="@+id/textViewTelefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="480dp"
        android:text="Telefono"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toTopOf="@+id/textViewCCorreo"
        app:layout_constraintEnd_toStartOf="@+id/editTextCorreo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textViewCCorreo"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.216" />

    <TextView
        android:id="@+id/textViewCCorreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:text="Correo Electronico"
        android:textColor="#F5F5F1"
        app:layout_constraintBottom_toTopOf="@+id/btnCrearCuenta"
        app:layout_constraintEnd_toStartOf="@+id/editTextCorreo"
        app:layout_constraintHorizontal_bias="0.969"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.941" />

    <Button
        android:id="@+id/btnCrearCuenta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Crear Cuenta"
        app:backgroundTint="#CEE50914"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.558"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.934" />

</androidx.constraintlayout.widget.ConstraintLayout>