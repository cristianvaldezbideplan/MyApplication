<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PantallaAcercaNosotros"
    android:background="#000000">

    <TextView
        android:id="@+id/textViewText3"
        android:layout_width="363dp"
        android:layout_height="72dp"
        android:text="@string/textViewText3"
        android:textColor="#F5F5F1"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.849" />

    <TextView
        android:id="@+id/textViewTitulo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewTitulo2"
        android:textColor="#F5F5F1"
        android:textSize="19sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewText1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.826" />

    <TextView
        android:id="@+id/textViewText1"
        android:layout_width="203dp"
        android:layout_height="75dp"
        android:text="@string/textViewText1"
        android:textColor="#F5F5F1"
        android:textSize="15sp"
        app:emojiCompatEnabled="false"
        app:layout_constraintBottom_toTopOf="@+id/textViewTitulo3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.584"
        app:layout_constraintStart_toStartOf="@+id/textViewTitulo2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.775"
        app:textAllCaps="false" />

    <ImageView
        android:id="@+id/imageViewMarcaLogo"
        android:layout_width="214dp"
        android:layout_height="183dp"
        app:layout_constraintBottom_toTopOf="@+id/textViewTitulo3"
        app:layout_constraintEnd_toStartOf="@+id/textViewText1"
        app:layout_constraintHorizontal_bias="0.245"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewTitulo2"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@mipmap/ic_launcher_background" />

    <TextView
        android:id="@+id/textViewText2"
        android:layout_width="348dp"
        android:layout_height="134dp"
        android:text="@string/textViewText2"
        android:textColor="#F5F5F1"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewTitulo4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.902" />

    <TextView
        android:id="@+id/textViewTitulo3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="105dp"
        android:text="@string/textViewTitulo3"
        android:textColor="#F5F5F1"
        android:textSize="19sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewText2"
        app:layout_constraintEnd_toEndOf="@+id/textViewText2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.967" />

    <TextView
        android:id="@+id/textViewTitulo4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewTitulo4"
        android:textColor="#F5F5F1"
        android:textSize="19sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewText3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.384"
        app:layout_constraintStart_toStartOf="@+id/textViewText2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.981" />

    <TextView
        android:id="@+id/textViewTitulo1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textViewTitulo1"
        android:textColor="#F5F5F1"
        android:textSize="23sp"
        app:layout_constraintBottom_toTopOf="@+id/textViewTitulo2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.529" />

</androidx.constraintlayout.widget.ConstraintLayout>